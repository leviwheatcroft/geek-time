{
  "name": "geek-time",
  "version": "1.0.0",
  "description": "time tracking for spreadsheet geeks",
  "main": "dist",
  "author": "Levi Wheatcroft <levi@wht.cr>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rimraf dist && mkdir dist",
    "copyTypes": "copyfiles -u 2 src/types/*.d.ts dist/",
    "buildCli": "webpack --config webpack.cli.js",
    "buildPlugin": "webpack --config webpack.plugin.js && cp dist/manifest.js dev/",
    "browsersync": "node browsersync",
    "dev": "yarn run clean && yarn run copyTypes && concurrently --raw \"yarn run npm-watch\" "
  },
  "watch": {
    "buildCli": {
      "patterns": [
        "src/cli/**/*.*"
      ],
      "inherit": true
    }
  },
  "dependencies": {
    "@types/papaparse": "^5.0.3",
    "config": "^3.3.1",
    "debug": "^4.1.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.7",
    "mongoose-lean-id": "^0.2.0",
    "mongoose-paginate-v2": "^1.3.9",
    "nconf": "^0.10.0",
    "papaparse": "^5.2.0",
    "prettytable": "^0.3.1",
    "tiny-async-pool": "^1.1.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/mongoose": "^5.7.8",
    "@types/nconf": "^0.10.0",
    "@types/node": "^13.11.0",
    "@types/tiny-async-pool": "^1.0.0",
    "@types/yargs": "^15.0.4",
    "concurrently": "^5.1.0",
    "copyfiles": "^2.2.0",
    "css-loader": "^3.4.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "npm-watch": "^0.6.0",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3",
    "typescript-is": "^0.15.0",
    "typescript-transform-paths": "^1.1.14",
    "webpack": "^4.42.1",
    "webpack-bundle-size-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.11",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
